{
    "version": 2,
    "builds": [{ "src": "*.js", "use": "@vercel/node" }],
    "routes": [
      {
        "src": "/.*",
        "dest": "/index.html"
      },
      {
        "src": "/.*",
        "dest": "/Frontend/CSS/styles.css"
      },
      {
        "src": "/.*",
        "dest": "/Frontend/JS/indexFront.js"
      }
    ]
  }