{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      },
      {
        "src": "/Frontend/CSS/(.*)",
        "dest": "/Frontend/CSS/$1"
      },
      {
        "src": "/Frontend/JS/(.*)",
        "dest": "/Frontend/JS/$1"
      }
    ]
  }