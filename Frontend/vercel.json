{
    "version": 2,
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      },
      {
        "src": "/Frontend/CSS/styles.css",
        "dest": "/Frontend/CSS/styles.css",
        "headers": {
          "Content-Type": "text/css"
        }
      },
      {
        "src": "/Frontend/JS/indexFront.js",
        "dest": "/Frontend/JS/indexFront.js"
      }
    ]
  }